#ifdef __USE_CMSIS
#include "LPC17xx.h"
#endif

#include <cr_section_macros.h>

int state=0;
int LED_PIN = 22;
unsigned int * GPIO_FIODIR = 0x2009C000;
unsigned int * GPIO_FIOMASK = 0x2009C010;


void local_gpio_init(){
	*GPIO_FIODIR = 1<<LED_PIN;//1 de output
}

int main(void) {

	local_gpio_Init();

    while(1) {
        blickLed(state);
    }
    return 0 ;
}
